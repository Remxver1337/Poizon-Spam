# КОПИРУЙТЕ ЭТОТ ФАЙЛ В .env И ЗАПОЛНИТЕ

# Токен основного бота (получите у @BotFather)
BOT_TOKEN=ваш_токен_здесь

# Ваш Telegram ID (можно узнать у @userinfobot)
ADMIN_ID=ваш_id_здесь

# Домен от bothost (после активации HTTP-сервера)
YOUR_HOST=ваш-домен.bothost.ru

# Порт (обычно 443)
YOUR_PORT=443

# Режим работы
USE_WEBHOOK=True
USE_POLLING=False

# Уровень логирования
LOG_LEVEL=INFO